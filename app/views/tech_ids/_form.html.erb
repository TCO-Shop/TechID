<%= simple_form_for(@tech_id) do |f| %>
	<%= f.input :name %>
	<%= f.input :college %>
	<%= f.input :category %>
	<%= f.input :LO  %>
	<%= f.input :description %> 
	<%= f.input :tech_id, :as => :integer %>
  <%= f.input :pdf, :as => :file %>  
  <%= f.button :submit, :input_html => {:class => 'button'} %>      
<% end %>
